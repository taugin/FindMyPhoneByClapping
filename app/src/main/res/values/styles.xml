<resources>
    <style name="common_button" parent="@style/Widget.AppCompat.Button.Borderless">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">56dp</item>
        <item name="android:background">@drawable/bg_large_file_btn</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">18sp</item>
        <item name="android:gravity">center</item>
        <item name="android:clickable">true</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:textStyle">bold</item>
        <item name="android:layout_marginStart">8dp</item>
        <item name="android:layout_marginEnd">8dp</item>
        <item name="android:layout_marginBottom">8dp</item>
    </style>

    <style name="common_text_style">
<!--        <item name="android:fontFamily"></item>-->
    </style>
    <style name="common_text_style2">
        <!--        <item name="android:fontFamily"></item>-->
    </style>

    <style name="DialogStartBtn">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">#ff707070</item>
        <item name="android:background">@drawable/dialog_lbtn_selector</item>
        <item name="android:layout_width">0.0dip</item>
        <item name="android:layout_height">48.0dip</item>
        <item name="android:layout_weight">1.0</item>
    </style>

    <style name="DialogEndBtn">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">#ff108bfe</item>
        <item name="android:background">@drawable/dialog_rbtn_selector</item>
        <item name="android:layout_width">0.0dip</item>
        <item name="android:layout_height">48.0dip</item>
        <item name="android:layout_weight">1.0</item>
    </style>
</resources>
